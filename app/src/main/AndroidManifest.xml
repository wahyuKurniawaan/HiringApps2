<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wahyu.hiringapps2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".dashboard.home.detail.hire.HireActivity"></activity>
        <activity android:name=".dashboard.home.detail.DetailProfileActivity" />
        <activity android:name=".auth.LoginWithAuthActivity" />
        <activity android:name=".retrofit.LearnRetrofitActivity" />
        <activity android:name=".dashboard.profile.room.NoteListActivity" />
        <activity android:name=".dashboard.profile.room.AddNoteActivity" />
        <activity android:name=".dashboard.profile.editProfile.EditProfileActivity" />
        <activity android:name=".dashboard.profile.WebViewActivity" />
        <activity android:name=".onBoard.OnBoardActivity" />
        <activity android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".register.SignUpActivity" />
        <activity android:name=".login.SignInActivity" />
        <activity android:name=".dashboard.HomeActivity" />
    </application>

</manifest>